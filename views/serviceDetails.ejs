<%- include ('common/head')-%> <%- include ('common/sidebar')-%>
    <!-- ======== main-wrapper start =========== -->
    <main class="main-wrapper">
      <%- include ('common/navbar')-%>

        <!-- ========== tab components start ========== -->
        <section class="tab-components">
          <div class="container-fluid">
            <!-- ========== title-wrapper start ========== -->
            <div class="title-wrapper pt-30">
              <div class="row align-items-center">
                <div class="col-md-6">
                  <div class="title">
                    <h2>Service Details Elements</h2>
                  </div>
                </div>
                <!-- end col -->
                <!-- <div class="col-md-6">
            <div class="breadcrumb-wrapper">
              <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                  <li class="breadcrumb-item">
                    <a href="#0">Dashboard</a>
                  </li>
                  <li class="breadcrumb-item"><a href="#0">Forms</a></li>
                  <li class="breadcrumb-item active" aria-current="page">
                    Form Elements
                  </li>
                </ol>
              </nav>
            </div>
          </div> -->
                <!-- end col -->
              </div>
              <!-- end row -->
            </div>
            <!-- ========== title-wrapper end ========== -->

            <!-- ========== form-elements-wrapper start ========== -->
            <div class="form-elements-wrapper">
              <div class="row">
                <div class="col-lg-6">
                  <!-- input style start -->

                  <div class="card-style mb-30">
                    <h6 class="mb-25">Input Fields</h6>
                    <form action="/admin/addService-details" method="post" enctype="multipart/form-data">

                      <div class="input-style-1">

                        <label>Service</label>
                        <select name="serviceId" class="form-control">
                          <option disabled selected>Select Service</option>
                          <% for(let data2 of serviceData){ %>
                            <option value="<%= data2._id %>">
                              <%= data2.title %>
                            </option>
                            <% } %>
                        </select>
                      </div>
                      <div class="input-style-1">
                        <label>Plans</label>
                        <select name="plansId" class="form-control">
                          <option disabled selected>Select Plans</option>
                          <% for(let data3 of planDetailsData){ %>
                            <option value="<%= data3._id %>">
                              <%= data3.serviceplan_name %>
                            </option>
                            <% } %>
                        </select>
                      </div>

                      <!-- end input -->
                      <div class="input-style-3">
                        <input type="text" name="title" placeholder="Title" required>
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>

                      <div class="input-style-3">
                        <input type="text" name="subtitle" placeholder="Subtitle" required>
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>

                      <div class="input-style-3">
                        <textarea name="about" placeholder="About" rows="5" required></textarea>
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>

                      <div class="input-style-3">
                        <input type="text" name="technology1" placeholder="Technology 1">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>

                      <div class="input-style-3">
                        <input type="text" name="technology2" placeholder="Technology 2">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>


                      <div class="input-style-3">
                        <input type="text" name="technology3" placeholder="Technology 3">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>


                      <div class="input-style-3">
                        <input type="text" name="technology4" placeholder="Technology 4">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>


                      <div class="input-style-3">
                        <input type="text" name="technology5" placeholder="Technology 5">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>


                      <div class="input-style-3">
                        <input type="text" name="technology6" placeholder="Technology 6">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>
                      <div class="input-style-3">
                        <input type="text" name="technology7" placeholder="Technology 7">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>
                      <div class="input-style-3">
                        <input type="text" name="technology8" placeholder="Technology 8">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>

                      <div class="input-style-3">
                        <input type="file" name="image" required multiple placeholder="Technology Image">
                        <span class="icon"><i class="lni lni-text-format"></i></span>
                      </div>

                      <!-- end input -->
                      <input class="main-btn active-btn rounded-full btn-hover" value="Submit" type="submit" />
                    </form>
                  </div>

                  <!-- end card -->
                  <!-- ======= input style end ======= -->
                </div>
                <!-- end col -->
                <div class="col-lg-6">
                  <!-- ======= textarea style start ======= -->
                  <div class="card-style mb-30">
                    <h6 class="mb-10">Data Table</h6>
                    <!-- <p class="text-sm mb-20">
                        For basic styling—light padding and only horizontal dividers—use
                        the class table.
                      </p> -->
                    <div class="table-wrapper table-responsive">
                      <table class="table">
                        <thead>
                          <tr>
                            <th class="lead-img">
                              <h6>Icon</h6>
                            </th>
                            <th class="lead-title">
                              <h6>Title</h6>
                            </th>
                            <th class="lead-description">
                              <h6>Description</h6>
                            </th>
                            <th>
                              <h6>Action</h6>
                            </th>
                          </tr>
                          <!-- end table row-->
                        </thead>

                        <% if(singleservice.length>0) {%>
                          <tbody>
                            <% for(let ser of singleservice) {%>
                              <tr>
                                <td class="min-width">
                                  <div class="lead">
                                    <div class="lead-image">
                                      <img src="/<%= ser.image%>" alt="" />
                                    </div>
                                  </div>
                                </td>
                                <td class="min-width">
                                  <p><a href="#0">
                                      <%= ser.title%>
                                    </a></p>
                                </td>
                                <td class="min-width">
                                  <p>
                                    <%= ser.subtitle%>
                                  </p>
                                </td>
                                <td class="min-width">
                                  <p>
                                    <%= ser.about%>
                                  </p>
                                </td>
                                <td>
                                </td>
                                <td class="min-width">
                                  <a href="/admin/serviceDetails-edit/<%=ser._id%>" class="status-btn success-btn">Edit</a>
                                </td>
                              </tr>
                              <!-- end table row -->
                          </tbody>
                          <%}} else {%> No Data Found
                            <% } %>
                      </table>
                      <!-- end table -->
                    </div>
                  </div>
                  <!-- ======= textarea style end ======= -->
                </div>
                <!-- end col -->
              </div>
              <!-- end row -->
            </div>
            <!-- ========== form-elements-wrapper end ========== -->
          </div>
          <!-- end container -->
        </section>
        <!-- ========== tab components end ========== -->
        <%- include ('common/footer')-%>
    </main>
    <!-- ======== main-wrapper end =========== -->
    <%- include ('common/foot')-%>