<div class="form-elements-wrapper">
    <div class="row">
      <div class="col-lg-6">
        <!-- input style start -->

        <div class="card-style mb-30">
          <h6 class="mb-25">Input Fields</h6>
          <form method="post" action="/admin/service-update">
            <input type="hidden" name="serviceId" value="<%=serED._id%>">
            <div class="input-style-1">
              <!-- <label>Service Title</label> -->
              <input type="text" name="title" placeholder="Service Title" value="<%=serED.title%>" />
            </div>
            <!-- end input -->
            <div class="input-style-3">
              <input type="text" name="service_desc" placeholder="Description" rows="5" value="<%=serED.service_desc%>"></input>
              <span class="icon"><i class="lni lni-text-format"></i></span>
            </div>
            <div class="input-style-2">
              <input type="file" name="image" placeholder="Icon" value="<%=serED.image%>" />
              <span class="icon"> <i class="lni lni-photo"></i> </span>
            </div>
            <!-- end input -->
            <input class="main-btn active-btn rounded-full btn-hover" value="Submit" type="submit" />
          </form>
        </div>

        <!-- end card -->
        <!-- ======= input style end ======= -->
